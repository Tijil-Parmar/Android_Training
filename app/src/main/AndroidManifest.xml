<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.StudyGoals">

    <application
        android:name=".DailyNotification.StudyGoalNotification"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SSCopy">
        <activity android:name="com.example.StudyGoals.getStarted"></activity>
        <activity
            android:name="com.example.StudyGoals.StudyModel.StudyActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        <receiver android:name=".DailyNotification.NotificationBroadcast"/>
    </application>

</manifest>